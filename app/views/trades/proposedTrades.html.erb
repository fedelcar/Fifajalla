<div class="container">
	<div class="row">
		<h1>Proposed Trades</h1>
	</div>
	
	<table class="table table-striped table-hover ">
    	<thead><tr>
    		<th>Nombre</th>
    		<th>User Origen</th>
    		<th>Equipo Origen</th>
    		<th>User Destino</th>
    		<th>Equipo Destino</th>
    		<th>Fecha</th>
    	</tr></thead>
    	
  		<% @trades.each do |trade| %>
  		<tbody><tr>

  			<td><%= trade.created_at.to_date %></td>
  		</tr></tbody>
  		<% end %>
  	</table>


	
	
</div>
